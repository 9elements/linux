								regulator-compatible = "vout2";
								regulator-name = "bcm2";
								regulator-boot-on;
							};
						};
					};
				};
				i2c@7 { // SMB_BCM3
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <7>;
					pli1209bc_p12v_d: regulator@5f {
