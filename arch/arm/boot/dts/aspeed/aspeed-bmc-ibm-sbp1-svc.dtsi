							p12v_b: vout2 {
								regulator-compatible = "vout2";
								regulator-name = "bcm1";
								regulator-boot-on;
							};
						};
					};
				};
				i2c@6 { // SMB_BCM2
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <6>;
					pli1209bc_p12v_c: regulator@5f {
						compatible = "vicor,pli1209bc";
